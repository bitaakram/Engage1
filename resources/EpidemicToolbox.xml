<xml xmlns="http://www.w3.org/1999/xhtml" id="toolbox" style="display: none;">
  <category name="Activity 1">
    <block type="persontype">
      <field name="Type">Man1</field>
    </block>
    <block type="status">
      <field name="Status">Healthy</field>
    </block>
    <block type="age">
      <field name="age">0</field>
    </block>
    <block type="in_behaviour"></block>
    <block type="move"></block>
    <block type="direction">
      <field name="Direction">Left</field>
    </block>
    <block type="simulation"></block>
    <block type="createMulti">
      <field name="Num">0</field>
      <field name="Types">People</field>
    </block>
  </category>
  <category name="Activity 2">
    <block type="virustype">
      <field name="Type">Virus1</field>
    </block>
    <block type="collision">
      <field name="NAME">Virus</field>
    </block>
    <block type="set2">
      <field name="Characteristic">Status</field>
    </block>
  </category>
  <category name="Activity 3">
    <block type="collision">
      <field name="NAME">Person</field>
    </block>
    <block type="characteristic_of">
      <field name="Characteristic">Status</field>
      <field name="Target">Collidee</field>
    </block>
  </category>
  <category name="Agents">
    <block type="persontype">
      <field name="Type">Man1</field>
    </block>
    <block type="status">
      <field name="Status">Healthy</field>
    </block>
    <block type="age">
      <field name="age">0</field>
    </block>
    <block type="virustype">
      <field name="Type">Virus1</field>
    </block>
  </category>
  <category name="Behavior">
    <block type="in_behaviour"></block>
    <block type="move"></block>
    <block type="direction">
      <field name="Direction">Left</field>
    </block>
    <block type="collision">
      <field name="NAME">Virus</field>
    </block>
    <block type="set2">
      <field name="Characteristic">Status</field>
    </block>
    <block type="characteristic_of">
      <field name="Characteristic">Status</field>
      <field name="Target">Collidee</field>
    </block>
  </category>
  <category name="Simulation">
    <block type="simulation"></block>
    <block type="createMulti">
      <field name="Num">0</field>
      <field name="Types">People</field>
    </block>
  </category>
  <category name="General">
    <block type="controls_if"></block>
    <block type="logic_compare">
      <field name="OP">EQ</field>
    </block>
    <block type="logic_operation">
      <field name="OP">AND</field>
    </block>
    <block type="math_arithmetic">
      <field name="OP">ADD</field>
      <value name="A">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="B">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="math_number">
      <field name="NUM">0</field>
    </block>
    <block type="math_random_int">
      <value name="FROM">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="TO">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
    </block>
    <block type="controls_repeat_ext">
      <value name="TIMES">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
  </category>
</xml>